{
  "headers": [
    {
      "source": "/(.+)",
      "headers" : [
        {
          "key" : "Cache-Control",
          "value" : "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(index\\.html|manifest\\.json|favicon\\.ico|images\/icons\/.*)",
      "headers" : [
        {
          "key" : "Cache-Control",
          "value" : "public, max-age=0, must-revalidate"
        }
      ]
    }
  ]
}
