{
  "headers": [
    {
      "source": "/index.html",
      "headers" : [
        {
          "key" : "Cache-Control",
          "value" : "public, no-store"
        }
      ]
    },
    {
      "source": "/manifest.json",
      "headers" : [
        {
          "key" : "Cache-Control",
          "value" : "public, no-store"
        }
      ]
    },
    {
      "source": "/((?!.*\\.html|.*manifest\\.json).*)",
      "headers" : [
        {
          "key" : "Cache-Control",
          "value" : "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
