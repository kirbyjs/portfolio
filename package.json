  "license": "MIT",
  "scripts": {
    "build": "rm -rf ./assets/bundle && cross-env NODE_ENV=production npm run webpack:prod",
    "start": "npm run webpack:dev",
    "webpack:dev": "webpack-dev-server --config ./webpack/config/dev-server.js --hot",
    "webpack:prod": "webpack --config ./webpack/config/app.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kirbyjs/portfolio.git"
  },
  "dependencies": {
    "lazysizes": "^5.2.0"
  },
  "devDependencies": {
    "autoprefixer": "~9.7.4",
    "cross-env": "~6.0.3",
    "css-loader": "~3.4.2",
    "file-loader": "^5.0.2",
    "html-webpack-plugin": "~3.2.0",
    "mini-css-extract-plugin": "~0.9.0",
    "optimize-css-assets-webpack-plugin": "~5.0.3",
    "postcss-loader": "~3.0.0",
    "sass": "~1.25.0",
    "sass-loader": "^8.0.2",
    "terser-webpack-plugin": "~2.3.2",
    "webpack": "~4.41.5",
    "webpack-cli": "~3.3.10",
    "webpack-dev-server": "~3.10.1"
  }
}
